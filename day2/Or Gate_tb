// Code your testbench here
// or browse Examples
module or_tb;
  reg a,b;
  wire y;
  or_1 dut (.a(a),.b(b),.y(y));
  initial begin 
    $monitor("a=%b, b =%b, y=%b",a,b,y);
    a = 0;b=0; #10
    a=0;b = 1;#10
    a =1;b=0;#10
    a=1;b=1;
  end 
  
  dumpfile("or_tb.vme d");
  dumpvars(1);
  endmodule
  
